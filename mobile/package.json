{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:ios:dev": "eas build --profile development --platform ios",
    "build:android:dev": "eas build --profile development --platform android",
    "build:ios:preview": "eas build --profile preview --platform ios",
    "build:android:preview": "eas build --profile preview --platform android",
    "build:ios:prod": "eas build --profile production --platform ios",
    "build:android:prod": "eas build --profile production --platform android",
    "build:all:prod": "eas build --profile production --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:all": "eas submit --platform all",
    "build:ios:submit": "eas build --profile production --platform ios --auto-submit",
    "build:android:submit": "eas build --profile production --platform android --auto-submit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-picker/picker": "^2.11.1",
    "expo": "~54.0.31",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "private": true
}
